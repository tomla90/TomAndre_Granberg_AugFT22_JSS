<!DOCTYPE html>
<html lang="en">

<head>
  <title>Meme Info</title>
  <%- include('./partials/head.ejs') %>
</head>

<body class="position-relative" data-bs-spy="scroll">
  <%- include('./partials/navbar.ejs') %>
<section class="container my-meme-section">


<div>
  <h1><%= meme.name %></h1>
  <img src="<%= meme.url %>" alt="<%= meme.name %>" style="max-width: 100%; height: auto;">
  <p>Size: <%= meme.width +"x"+ meme.height %></p>
  <p>ID: <%= meme.id %></p>
  <p>URL: <%= meme.url %></p>
</div>

</section>
<%- include('./partials/scripts.ejs') %>
</body>
</html>