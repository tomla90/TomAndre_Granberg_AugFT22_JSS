<!DOCTYPE html>
<html lang="en">

<head>
  <title>Portfolio</title>
  <!-- BM Chaneg the path to the css files -->
  <link rel="stylesheet" href="../../css/bootstrap.min.css" />
  <link href="../../stylesheets/navbar.css" rel="stylesheet">

</head>

<body class="position-relative" data-bs-spy="scroll">
  <%- include('./partials/navbar.ejs') %>
    <section class="container">
      <table class="table" id="table">
        <thead>
          <tr>
            <th scope="col">ID</th>
            <th scope="col">Size</th>
            <th scope="col">Image</th>
            <th scope="col">Options</th>
          </tr>
        </thead>
        <tbody>

          <tbody>
            <% for(let i = 0; i < data.length && i < 100; i++) { %>
                <tr>
                    <th scope="row"><%= i + 1 %></th>
                    <td><%= data[i].name %></td>
                    <td><img src="<%= data[i].url %>" alt="<%= data[i].name %>" width="100" height="100"></td>
                    <td>
                        <a href="<%= data[i].url %>" target="_blank" class="btn btn-primary">View</a>
                        <a href="<%= data[i].page_url %>" target="_blank" class="btn btn-secondary">Page</a>
                    </td>
                </tr>
            <% } %>
        </tbody>
        </tbody>
      </table>
    </section>
    <%- include('./partials/scripts.ejs') %>
</body>

</html>